            <p class=
            "hfwmm-tertiary-list-title hfwmm-light-tertiary-list-title">TOP
            STORIES</p>

            <ul class="hfwmm-list hfwmm-4uphp-list hfwmm-light-list"
            data-track-prefix="flex4uphphero">
              <%= yield %>
            </ul>

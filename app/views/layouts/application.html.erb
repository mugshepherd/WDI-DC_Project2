<body class="site-theme-usatoday">
  <header class=" site-header" data-default-fixed="false" data-default-open=
  "true">
    <div class="site-header-inner-wrap">
      <div class="site-masthead">
        <div id="animated-site-logo"><a data-ht="Headeranimatedlogodot" href=
        "/" id="animated-site-logo-dot-link"></a><a class=
        "animated-site-logo-link" data-ht=
        "Headerusatoday Headeranimatedlogolink" href="/"></a><img alt=
        "USA Today" src=
        "http://i.usatoday.net/Dynamic_logos%5C2015-04-21-purplelemon%5C042015-purplelemon.png"
        style="position:absolute"></div>

        <div class="site-masthead-search-wrap">
          <form action="/search/" class=
          "site-masthead-search-form ui-ajax-form" data-mapping="q" method=
          "get">
            <div class=
            "site-masthead-search-input-wrap ui-form-field-container">
              <input autocomplete="off" class=
              "site-masthead-search-form-input ui-text-input" data-clearable=
              "true" name="q" placeholder="Search" type="text">

              <div class=
              "site-masthead-search-btn site-masthead-search-btn-div" data-ht=
              "Headersearchbutton"></div>
            </div><input class="site-masthead-search-btn submit" type="submit"
            value="Search"><span class="site-masthead-search-close-btn"
            data-uotrack="Headersearchclosebutton"></span>

            <div class=
            "site-masthead-subscribe-wrap site-masthead-subscribe-wrap-visible">
            <a class="sam-returns site-masthead-subscribe-link" data-ht=
            "Headersubscribecallout" href=
            "https://offers.usatoday.com/3FOR34COM?opm_position=masthead&amp;opm_creative=blue-white-text&amp;opm_name=subcribe-for-home-delivery&amp;opm_id=64&amp;keycode=wucag"
              target="_blank"><span class=
              "site-masthead-subscribe-primary-text">SUBSCRIBE
              NOW</span><span class="site-masthead-subscribe-secondary-text">to
              get home delivery</span></a>
            </div>
          </form>
        </div>
      </div>
      <%= render "navbar" %>
    </div>
  </header>

  <article class="cards cards-primary-flex-sidebar-page" id="cards">
    <div class="card-container card-container-primary-flex-sidebar-page">
      <div class="card-wrap card-wrap-primary-flex-sidebar-page">
